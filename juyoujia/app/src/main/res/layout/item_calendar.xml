<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.idougong.jyj.widget.ImageView_126_150
        android:id="@+id/iv_signbg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@mipmap/sign_off"/>
    <TextView
        android:id="@+id/tv_calendar"
        android:layout_width="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_10sp"
        android:layout_marginTop="10dip"
        android:textColor="@color/color_sign3" />
    <TextView
        android:id="@+id/tv_calendar_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/iv_signbg"
        android:gravity="center"
        android:textSize="@dimen/text_12sp"
        android:textColor="@color/tabwd"
        android:layout_marginTop="5dip"/>
</RelativeLayout>